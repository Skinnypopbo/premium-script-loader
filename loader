--Game ids
local gameId = game.PlaceId
local Inkgame = 99567941238278
local ink = 125009265613167
local ArcadeBasketBall = 134558434771720

if gameId == Inkgame then
	loadstring(game:HttpGet("https://raw.githubusercontent.com/Skinnypopbo/Ink-Game/refs/heads/main/Ink"))()
else if gameId == ink then
	loadstring(game:HttpGet("https://raw.githubusercontent.com/Skinnypopbo/Ink-Game/refs/heads/main/Ink"))()
else if gameId == ArcadeBasketBall then
	loadstring(game:HttpGet("https://raw.githubusercontent.com/Skinnypopbo/Arcade-BasketBall/refs/heads/main/Arcade"))()

--if you get this your just dumb and slow :(
else if gameId == nil then
	local CoreGui = game:GetService("StarterGui")

	CoreGui:SetCore("SendNotification", {
	Title = "Not Supported :(";
	Text = "";
	Duration = 5;
	})
	end
      end
  end
end
